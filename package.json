{
  "name": "battle-angel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "client": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "clean:server": "rimraf server/server-dist",
    "clean:client": "rimraf server/server-dist",
    "build:server": "npm run clean:server && cross-env NODE_ENV=production webpack --config server/build/webpack.server.config.js",
    "build:client": "npm run clean:client && cross-env NODE_ENV=production webpack --config server/build/webpack.client.config.js",
    "lint": "vue-cli-service lint",
    "server": "node server/index.js"
  },
  "dependencies": {
    "ant-design-vue": "^1.3.7",
    "axios": "^0.18.0",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "memory-fs": "^0.4.1",
    "vue": "^2.6.6",
    "vue-class-component": "^7.0.1",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.4",
    "@vue/babel-preset-jsx": "^1.0.0-beta.2",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "copy-webpack-plugin": "^5.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "vue-loader": "^15.7.0",
    "vue-server-renderer": "^2.6.9",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": 0
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaFeatures": {
        "legacyDecorators": true
      }
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
